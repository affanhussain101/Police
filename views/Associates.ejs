<!DOCTYPE html>
<html lang="en">
<head>
  <title>Associates</title>
  <link rel="stylesheet" href="table.css">
</head>
<body>
    <div class="table-data">
<h2 class="tab">List Of Associates</h2>
    <table>
         <tr>
             <!-- <th>S.No</th>  -->
            <th>POLICE_ID</th>
            <th>Full Name</th>
            <th>Designation</th>
            <th>CONTACT_NO</th>
            <th>Station_Id</th>
            <th>Delete</th>
        </tr>
        
        <%
        if(Associates.length!=0){
        var i=1;
        Associates.forEach(function(data){
        %>
        <tr>
            <!-- <td><%=i; %></td> -->
            <td><%=data.POLICE_ID %></td>
            <td><%=data.NAME %></td>
            <td><%=data.DESIGNATION %></td>
            <td><%=data.CONTACT_NO %></td>
            <td><%=data.STATION_ID %></td>
            <td><a href="/Associates/delete/<%=data.POLICE_ID%>"><button class="tabButton">Delete</button></a></td>
        </tr>
        <%  i++; }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
    </div>
</body>
</html>